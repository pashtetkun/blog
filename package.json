{
  "name": "blog",
  "version": "1.0.0",
  "description": "Mini blog",
  "main": "server.js",
  "scripts": {
    "compile-js": "browserify app/components/index.js -o server/src/main/resources/public/js/app.js -t [ babelify --presets [ es2015 react ] ] -v",
    "watch-js": "watchify app/components/index.js -o server/src/main/resources/public/js/app.js -t [ babelify --presets [ es2015 react ] ] -v"
  },
  "author": "kot",
  "license": "ISC",
  "dependencies": {
    "jquery": "3.2.1",
    "markdown": "0.5.0",
    "react": "^15.5.4",
    "react-dom": "15.5.4",
    "react-images": "0.5.2",
    "react-router": "3.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "nodemon": "1.11.0",
    "watchify": "3.9.0"
  }
}
